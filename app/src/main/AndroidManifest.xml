<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.estacionamientolaglorieta">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EstacionamientoLaGlorieta">
        <activity android:name=".Registros"></activity>
        <activity android:name=".Vehiculos" />
        <activity android:name=".CuentaUsuario" />
        <activity android:name=".RegistrarCliente" />
        <activity android:name=".InicioSesion" />
        <activity
